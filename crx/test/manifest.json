{
  "name": "Time Tracker",
  "version": "0.1",
  "description": "Compete against friends to be the most productive.",
  "icons": { "128": "news_icon.png" },
  "browser_action": {
    "default_title": "Time Tracker",
    "default_icon": "news_action.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "manifest_version": 2
}
